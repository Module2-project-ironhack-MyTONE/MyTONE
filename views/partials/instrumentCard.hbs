
<a class="card-link" href="/instruments/{{_id}}">
	<article class="card">
			<div class="card-inner">
				<div class="card-image">
					<img src={{image}} alt={{title}} />
				</div>
				<div class="card-content">
					<div class="card-meta">
						<span class="card-meta-number">Production year: {{year}}. Made in {{madeIn}}.</span>
			
						<button class="card-user-button">	
              {{!-- <img src={{owner.image}} alt={{owner.title}} /> --}}
						</button>
				</div>
					<h2 class="card-title">{{brand}} {{model}}</h2>
				</div>
			</div>
		</article>
</a>